#!/bin/bash
#================================================
GPWD=$PWD
#================================================
. $GPATH/.config
#================================================
lCheckIn="GPATH" ; if [ "${!lCheckIn}" = "" ] || ! [ -d "${!lCheckIn}" ] ; then echo "Erreur: $lCheckIn ?" ; exit ; fi 
#================================================
modprobe -a vboxguest vboxsf vboxvideo
#================================================
cd $GPWD
#================================================
